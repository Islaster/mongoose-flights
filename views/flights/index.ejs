<!DOCTYPE html>
<html lang="en">
  <%- include('../partials/head') %>
    <table>
      <thead>
        <tr>
          <th>Airline</th>
          <th>Airport</th>
          <th>Flight No.</th>
          <th>departs</th>
        </tr>
      </thead>
      <tbody>
        <% flights.forEach(function (f) {%>
        <tr>
          <td><%= f.airline%></td>
          <td><%= f.airport%></td>
          <td><%= f.flightNo%></td>
          <td><%= f.departs%></td>
          <td><a href="/flights/<%= f._id%>">details</a></td>
        </tr>
        <%});%>
      </tbody>
    </table>
  </body>
</html>
